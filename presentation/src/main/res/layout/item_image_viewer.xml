<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="image"
            type="String" />
        <variable
            name="isCacheFile"
            type="Boolean" />
    </data>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        loadUrl="@{image}"
        isFile="@{isCacheFile}"
        android:scaleType="fitXY"/>
</layout>
